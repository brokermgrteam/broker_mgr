$("#brokerteam_url").html("<%= escape_javascript(render('manager_modal')) %>")
$('#managerModal').modal('show')
$("td[id^='brokerteamrelstatus']").each(function() {
  var id = parseInt(this.id.replace("brokerteamrelstatus", ""), 10);
  var tdid = "#brokerteamrelstatus" + id;
  var tdidclick = "#click" + id + " a";
  $(tdidclick).click( function() {
    $(tdid).html('<%= "#{get_dict("BrokerTeamrel.status", 1).name}" %>')
  });
});
