<div class="row-fluid">
  <!-- <div class="span6"> -->
    <%= f.input :channel, :label => '选择渠道', :collection => Channel.valid_channels.accessible_by(current_ability).order('channel_code'),
            :include_blank => true, :required => true,  input_html: { :class => "channel_selection" } %>
  <!-- </div> -->
<!-- </div> -->
<!-- <div class="row-fluid"> -->
  <!-- <div class="span6"> -->
    <%= f.input :cust_branch, :label => '开户营业部', :collection => Branch.accessible_by(current_ability).map{|br| ["#{br.code} | #{br.name}", br.id]},
            :include_blank => true,  input_html: { :class => "branch_selection" } %>
  <!-- </div> -->
  <!-- <div class="span6"> -->
    <%= f.input :channel_branch, :label => '服务营业部', :collection => Branch.accessible_by(current_ability).map{|br| ["#{br.code} | #{br.name}", br.id]},
            :include_blank => true %>
    <%= f.input :sub_channel, :label => '二级渠道', input_html: { :id => "subchannel_text", autocomplete: 'off', 'data-provide' => 'typeahead' } %>
  <!-- </div> -->
<!-- </div> -->
<!-- <div class="row-fluid"> -->
  <!-- <div class="span6"> -->
    <%= f.input :broker, :label => '营销人员', :collection => [], as: :select,
            :include_blank => true, :required => false, input_html: { :class => "broker_selection" } %>
  <!-- </div> -->
    <%= f.input :bank_no, :label => '存管银行', :collection => Bank.accessible_by(current_ability).order('bank_code'),
            :include_blank => true, :required => false %>
</div>
